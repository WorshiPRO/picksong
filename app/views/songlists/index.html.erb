<p id="notice"><%= notice %></p>

<h1>Songlists</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Key</th>
      <th>Band</th>
      <th>Url</th>
      <th>Lyrics</th>
      <th>Category</th>
      <th>Mark</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @songlists.each do |songlist| %>
      <tr>
        <td><%= songlist.name %></td>
        <td><%= songlist.key %></td>
        <td><%= songlist.band %></td>
        <td><%= songlist.url %></td>
        <td><%= songlist.lyrics %></td>
        <td><%= songlist.category %></td>
        <td><%= songlist.mark %></td>
        <td><%= songlist.user %></td>
        <td><%= link_to 'Show', songlist %></td>
        <td><%= link_to 'Edit', edit_songlist_path(songlist) %></td>
        <td><%= link_to 'Destroy', songlist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Songlist', new_songlist_path %>
